
@font-face {
    font-family: "SegoeWP Light" ;
    src: url("../fonts/SegoeWP-Light.ttf");
}
@font-face {
    font-family: "SegoeWP Bold" ;
    src: url("../fonts/segoe%20wp-bold.ttf");
}
@font-face {
    font-family: "SegoeWP";
    src: url("../fonts/SegoeWP.ttf");
}
@font-face {
    font-family: "Segoe UI Symbol";
    src: url("../fonts/seguisym.ttf");
}
@font-face {
    font-family: "SegoeWPN Semilight";
    src: url("../fonts/SegoeWPN-Semilight.ttf");
}
@font-face {
    font-family: "Helveticaltstd-roman";
    src: url("../fonts/helveticaltstd-roman.otf");
}
@font-face {
    font-family: Helveticaltstd-light;
    src: url("../fonts/helveticaltstd-light.otf");
}

*{margin: 0px; padding: 0px;}

body{ font-size:  13px; font-family: SegoeWP, arial, sans-serif;}

ul {list-style:  none; }

a {text-decoration:  none; color: #555454; }

/*-----------HEADER--------*/
.header {width: 100%;  }
.header .box {width: 998px; margin: auto;}
.header .top {height:  30px; width: 100%; background: #424242;}
.top ul {list-style:  none; float: right;}
.top ul li {display: inline-block; height:  30px; line-height:  30px; padding-right: 20px;}
.top ul li a {color: #fff;  font-family: "SegoeWP Light", arial, sans-serif;font-size:  16px;}

/**************main_header************/
.main_header {background: url("../images/bg_main_header.png") repeat-x;overflow:  hidden;}
.main_header .box_main_header{ width: 963px; margin:  auto; padding: 20px 20px 0px 15px;  height: 90px; }
.box_main_header {overflow: hidden;}
.box_main_header h1 {float: left;}
.box_main_header form {height:  76px; line-height:  76px; margin-left: 40px;  width: 460px; float: left; position: relative}
.box_main_header form input[type="text"] { padding-left: 6px; width:  327px; height:  40px; border: none; background: url("../images/input_search.png") no-repeat; outline:  none;}
.box_main_header form input[type="button"] {width: 124px; height: 40px; border: none; background: url("../images/btn_search.png") no-repeat; position: absolute;top: 18px; right: 3px; outline: none; cursor: pointer}
.box_main_header .cart {float: right; margin-left: 20px;  min-height:  70px;}
.box_main_header .cart .col1 {float: left; width:  190px; border-right: 1px solid #c6c6c6; min-height:  70px;}
.cart .col1 p {font-size: 13px; color: #6d6d6d; height:  30px; line-height:  30px; }
.cart .col1 p img {margin-right: 5px;}
.cart .col1 p {font-family:  SegoeWP; font-size:  13px;}
.cart .col1 p.y_cart {background: url("../images/cart.png") no-repeat; padding-left: 30px;}
.cart .col1 .your_cart {font-size: 23px; line-height:  30px; height:  30px;margin-bottom: 10px; font-family: SegoeWP;}
.cart .col1 .price {font-size: 27px; font-family: "SegoeWP Bold";  float: left; margin-right:  13px;}
.cart .col1 a.checkout {background: url("../images/button.png") no-repeat; width:  83px; height:  28px;  border: none; font-size: 15px; color: #fff; font-family:  SegoeWP; display:  block; float: left; text-align:  center;}
.cart .col2 {padding-left: 10px; width:  93px; float: right;}
.cart .col2 img {margin-left: 25px;}
.cart .col2 .p_20 {margin-left: 35px; background: url("../images/wlist.png") 0% 50% no-repeat; width:  28px; height:  16px; line-height:  16px; text-align: center; color: #fff; margin-bottom: -10px; margin-top: -9px; margin-left: 60px;}
.cart .col2 .wish_list {font-size: 21px; font-family:  SegoeWP;}

/**************nav_menu************/
.nav_menu {border-top: 1px solid #cecece; border-bottom: 1px solid #cecece; height:  39px;}
.nav_menu ul {list-style:  none; }
.nav_menu ul li {display: inline-block; height:  41px; line-height:  41px; padding: 0px 15px; margin-right: 0px !important;}
.nav_menu ul li:hover {background: url("../images/bg_hover_menu.png") repeat-x;}
.nav_menu ul li.hover {background: url("../images/bg_hover_menu.png") repeat-x;}
.nav_menu ul li a {color: #5b5b5b; font-size:  19px; font-family: "Segoe UI Symbol", arial, sans-serif;}

/*-----------CONTENT--------*/
.content {margin-top: 10px;}
.content .box {width: 998px !important; margin: auto;}
.content .box #bxslider { width: 714px; height: 304px; float: left; margin-right: 10px; margin-bottom: 13px;}
.content .box .deal {float: left;}


/***************main_wrapper*******************/
.main_wrap {width: 100%; overflow:  hidden; clear: both;}
.main_wrap .sidebar{width: 188px; border: 1px solid #d6d6d6; float: left; margin-right: 7px;}
.main_wrap .sidebar h3 {margin-top: 12px; margin-left: 17px; font-size: 24px; font-family: "SegoeWP Light"; color: #6b6969; margin-bottom:  7px;}
.sidebar ul.ul_all {margin-left: 0px;}
.sidebar ul li.all {display:  block;  width:  168px; height:  30px; background: url("../images/all_bg.png") repeat-x; line-height:  30px; padding-left: 20px; border: none;}
.sidebar ul  li.all a {font-size: 15px; color: #fff; }
.sidebar h4 {margin: 15px 0px 0px 15px; color: #696969; line-height:  15px;}
.sidebar ul {margin-left: 24px;}
.sidebar ul a {color: #555454;}
.sidebar ul li {height:  28px; line-height:  28px; display:  block; width: 139px; border-bottom: 1px solid #e6e7e8;}

.primary {width:  770px; float: left; border: 1px solid #d6d6d6;margin-right: -5px; margin-top: 39px; padding: 20px 5px 19px 15px; margin-bottom: 10px;}
.primary .pagin { clear: both; text-align:  right; margin-right: 20px; overflow:  hidden; float: right; margin-top: 58px; margin-bottom: -20px;}
.primary .pagin a {display: block; width:  24px; height:  28px; float: left; text-align:  center; line-height:  28px; font-family: Helveticaltstd-light; font-size:  16px;}
.primary .pagin .selected {background: url("../images/bg_pagin.png") no-repeat ; }
.primary #tabs_products {position: relative;}
.primary .clear {clear: both;}
.primary ul {position:  absolute; top: -60px; left: -16px;}
.primary ul.ul_tab li {float: left;  }
.primary ul.ul_tab li.ui-tabs-active a { outline: none; background: url("../images/bg_active_tabs.png") repeat-x; padding-bottom: 1px; border-bottom: none;}
.primary ul.ul_tab li.ui-state-active a { outline:  none;}
.primary ul.ul_tab li a{display:  block; width:  102px; height:  38px; border: 1px solid #d6d6d6;  line-height:  38px;text-align: center; padding: 0px 20px; background: url("../images/bg_tabs.png") repeat-x;}
/******************list_product****************/
/*.primary #tabs_products #tabs-1, .primary #tabs_products #tabs-2, .primary #tabs_products #tabs-3{ overflow:  hidden; border: 1px solid red;}*/
 .div_product {width:  126px;  background: url("../images/product/product01.png") top center no-repeat; float: left; margin-right: 30px; position:  relative;}
 .div_product:nth-child(5),.div_product:nth-child(10),.div_product:nth-child(15),.div_product:nth-child(20) {margin-right: 0px;}
.div_product .off {display:  block; width: 50px; height:  50px; background: url("../images/off.png") no-repeat; position: absolute; top: 0px; right: 0px; line-height:  40px; font-size:  19px; color: #fff; font-weight:  bold; text-align:  center;}
 .div_product img {margin-top: 14px; margin-left: 14px;}
.div_product p {display:  block; text-align:  center; }
.div_product p a { display: block; text-align:  center; width: 98px; margin: 0px auto; font-size:  15px; font-family: "SegoeWPN Semilight"; color: #595959; margin-top: 10px;}
.div_product h4 { text-align: center; font-family:  helveticaltstd-roman; font-size:  21px; color: #f05356;}

/******************list_product_page****************/
#product_page {margin-top: 0px;}
#product_page h3 {font-size: 24px; font-family:  "SegoeWP Light"; color: #464646; margin-bottom:  20px;}

/******************detail_page****************/
.content .box {overflow:  hidden;}
.detail_jumlink {background: url("../images/bg_jumlink.png") no-repeat; height:  37px; width:  1002px; margin-top:  0px; margin-bottom:  5px; margin-left: -2px;}
.detail_jumlink p {height:  37px; line-height:  37px; margin-left: 20px;}
.detail_jumlink span.bg_jumlink {background: url("../images/detail_jumlink.png") 50% 50% no-repeat; display: inline-block; width: 6px; height:  10px; margin:  0px 10px;}
.feature {border: 1px solid #dedede; width:  938px; padding: 27px 29px; border-radius:  5px; -moz-border-radius: 5px; -webkit-border-radius: 5px; overflow:  hidden; position:  relative; margin-bottom:  20px;}
.feature img {float: left; margin-right: 40px;}
.feature h2 {font-size:  40px; font-family: "SegoeWP Light"; color: #595959; margin-bottom:  25px;}
.feature p {font-size:  16px; font-family: "Segoe UI Symbol"; color: #7e7e7e;}
.feature p.stock {position: absolute; top: 30px; right: 35px; font-size:  10px; font-family:  arial; color: #817f7f; background: url("../images/stock.png") 50% 50% no-repeat;  height:  30px; line-height:  30px; text-align:  center;}
.feature .btn_addtocart { position:  relative;width: 571px; height:  132px; border: 1px solid #d1d1d1; float: left; margin-top: 20px; padding: 20px;}
.feature .btn_addtocart p.our_price {font-size:  35px; color: #015699;}
.feature .btn_addtocart p.our_price .detail_price {color: #e45858;}
.feature .btn_addtocart p.sale {font-size: 17px; font-family: "Segoe UI Symbol"; color: #939292;}
.feature .btn_addtocart a.btn_cart {position: absolute; top: 20px; right: 30px; background: url('../images/btn_addtocart.png') no-repeat; width: 197px; height:  42px; color: #fff; font-size:  24px; text-align:  center; line-height:  42px; font-weight:  bold}
.feature .btn_addtocart .paypal {border-top: 1px solid #d1d1d1; margin-top:  20px;}
.feature .btn_addtocart .paypal .lock {background: url("../images/lock.png") 0% 50% no-repeat; padding-left: 20px; font-size:  13px; margin: 5px 0px;}
.feature .btn_addtocart .paypal img {margin-right: 5px;}

.desc {width: 730px; float: left;}
.desc #desc_tabs {margin-top: 60px; border: 1px solid #d5d2d2; border-radius:  0px 5px 5px 5px; -moz-border-radius:  0px 5px 5px 5px; -webkit-border-radius:  0px 5px 5px 5px; margin-bottom:  20px; padding: 20px; position:  relative;}
.desc #desc_tabs ul {position: absolute; top: -36px; left: -1px;}
.desc #desc_tabs ul li {display:  inline; float: left; margin-right: 10px;}
.desc #desc_tabs ul li a {display:  block;  width:  135px; height:  36px; background:  url("../images/desc_tabs.png") no-repeat; text-align:  center; line-height:  36px; font-size:  16px; font-family: "Segoe UI Symbol"; color: #fff;}
.desc #desc_tabs ul li.ui-tabs-active a {width: 208px; height: 35px; outline:  none; background:#fff; font-size:  18px; color: #7e7e7e; border: 1px solid #d5d2d2; border-radius:  5px 5px 0px 0px; -moz-border-radius:  5px 5px 0px 0px; -webkit-border-radius:  5px 5px 0px 0px; border-bottom:  none;}
.desc #desc_tabs #tabs-1, .desc #desc_tabs #tabs-2, .desc #desc_tabs #tabs-3 {color: #7e7e7e; font-size:  14px;}
.desc .comment {overflow: hidden;border: 1px solid #d5d2d2; border-radius:  5px; -moz-border-radius: 5px; -webkit-border-radius: 5px; margin-bottom:  20px; padding: 20px;}
.desc .comment .name {border-bottom: 1px solid #e3e3e3; overflow: hidden; padding: 20px 0px; }

/******************shopping_cart****************/
.shopping_cart table {width: 998px; }
.shopping_cart table thead {width: 100%;  background: #ddd;}
.shopping_cart table thead tr {height: 50px;}
.shopping_cart table tbody tr {text-align: center;}
.shopping_cart table tbody tr td {padding: 10px 0px; font-weight: bold;}
.shopping_cart table tbody tr td input[type="text"] {width: 30px;}
.shopping_cart table tfoot a {color: #0299aa;}
.shopping_cart table tfoot a:hover {text-decoration: underline}
.shopping_cart .table_box {text-align: right; margin: 20px 0px;}
.shopping_cart .table_box p {margin-bottom: 10px;}
.shopping_cart .table_box a { color: #0299aa;}
.shopping_cart .table_box a:hover{text-decoration: underline;}


/******************Check_out****************/
.check_out form {width: 100%;overflow:hidden;}
.check_out form .fl_l {width: 500px; float: left;}
.check_out form .fl_l label{display: block; margin: 10px 0px;}
.check_out form .fl_l input {width: 270px; padding: 5px; border: 1px solid #dedede; color: #808b98;}
.check_out form .fl_r {width: 498px; float: right;}
.check_out form .fl_r label{ display: block; margin: 10px 0px;}
.check_out form .fl_r input {width: 270px; padding: 5px; border: 1px solid #dedede; color: #808b98;}
.check_out form .fl_r textarea {width: 270px; padding: 30px; border: 1px solid #dedede; color: #808b98; }
.check_out .check_out_button{ height: 40px; clear: both; text-align: center;padding: 20px 0px;}
.check_out .check_out_button input[type="submit"]{font-family: "SegoeWP Bold";border: none; display: inline-block; width: 98px; height: 38px; background: url("../images/btn_submit.png") no-repeat; cursor: pointer; color: #555454;}
.check_out .check_out_button a {font-weight: bold;display: inline-block; width: 98px; height: 38px; background: url("../images/btn_submit.png") no-repeat; line-height: 38px;}

/******************Contact_page****************/
.contact_page {width: 100%; overflow: hidden;}
.contact_page form {float: left;width: 500px;}
.contact_page form label {display: block; margin: 10px 0px; }
.contact_page form input {width: 270px; padding: 5px; border: 1px solid #dedede; color: #808b98}
.contact_page form textarea {width: 270px; padding: 30px; border: 1px solid #dedede; color: #808b98; }
.contact_page form input[type="submit"] {font-family: "SegoeWP Bold";border: none;  width: 98px; height: 38px; background: url("../images/btn_submit.png") no-repeat; cursor: pointer; color: #555454;}
.contact_page .contact_info {float: right;}
/*.desc .comment .name:last-child {border-bottom: none;}*/
.desc .comment h4 {font-size: 20px; font-family: "Segoe UI Symbol"; color: #7e7e7e; }
.desc .comment .img {float: left; margin-right: 15px; }
.desc .comment .review {float: left;  width:  570px; font-size:  13px; font-family:  "Segoe UI Symbol"; color: #7e7e7e;}
.desc .comment .img img { margin-bottom:  5px;}
.desc .comment .img p {text-align:  center; color: #7e7e7e}
.desc .comment fieldset {border: none; margin-top: 20px;}
.desc .comment legend {font-size:  20px; font-family: "Segoe UI Symbol"; color: #7e7e7e; margin-bottom: 20px;}
.desc .comment label { font-size:  16px; font-family: "Segoe UI Symbol"; color: #a2a2a2; text-align: right; width: 90px; display: inline-block;}
.desc .comment p {margin-bottom: 20px;}
.desc .comment input[type="text"]{width: 548px; height: 30px; border: 1px solid #e2e2e2; border-radius:  5px; -moz-border-radius:  5px; -webkit-border-radius:  5px; margin-left: 20px;}
.desc .comment textarea {width: 548px; height: 129px; border-radius:  5px; -moz-border-radius: 5px; -webkit-border-radius: 5px; margin-left: 20px; border: 1px solid #e2e2e2;}
.desc .comment input[type="submit"]{border: none; display: block; width: 97px; height:  38px; background: url("../images/btn_submit.png") no-repeat; color: #fff; font-size:  18px; float: right; margin-right: 25px;}

.like {margin-top: 60px;margin-left: 10px; padding-bottom: 20px;border: 1px solid #dedede;width: 248px; float: left; border-radius:  5px; -moz-border-radius: 5px; -webkit-border-radius:  5px;}
.like h3 {margin-top: 16px; margin-left:  14px; font-size:  18px; font-family:  "Segoe UI Symbol";color: #7e7e7e;}
.like .like_product {border-bottom: 1px solid #dedede; overflow:  hidden;}
.like .like_product:last-child {border: none;}
.like .like_product .item {padding: 5px;}
.like .like_product .item a.img { display:  block; width: 81px; height:  109px;float: left;position: relative; background:  url("../images/bg_like_product.png") no-repeat;}
.like .like_product .item img {position: absolute; top: 15px; left: 15px;  width:  50px;}
.like .like_product .item div {float: left;}
.like .like_product .item h4 {font-size:  15px; color: #595959; font-weight:  normal; margin-top: 20px; }
.like .like_product .item p.like_price {font-size: 21px; color: #f05356; }
.like .like_product .item a.read_more {display:  block; width: 87px; height:  24px; background:  url("../images/read_more.png") no-repeat; font-size: 15px; color: #fff;font-family:  "Segoe UI Symbol"; line-height:  24px; text-align:  center;}
/*-----------FOOTER--------*/

/***********main_footer***********/
.footer {overflow: hidden; clear: both;}
.main_footer {background: url("../images/bg_footer.png"); height:  335px; padding-top: 20px; }
.main_footer .box {width: 998px; margin:  auto; overflow: hidden;}
.main_footer .box .col {float: left; padding-right: 20px; }
.main_footer .box .col h4 {font-size: 20px; font-family: "SegoeWP Light"; color: #dddddd; margin-bottom: 15px;}
.main_footer .box .col li { border-bottom: 1px solid #565757; height:  24px; line-height:  24px;  }
.main_footer .box .col li:last-child {border-bottom:  none;}
.main_footer .box .col a {font-size: 14px; font-family: "SegoeWP Light"; color:  #ddd;}
.main_footer .box #col1  {width: 236px; }
.main_footer .box #col2 li {width: 250px; }
.main_footer .box #col3 {width: 174px; }
.main_footer .box #col4 {width: 200px;}

/***********footer_info***********/
.footer_info {clear: both;background: url("../images/bg_footer_info.png"); height:  121px;font-size:  20px; font-family: "SegoeWP Light"; color: #dddddd; text-align:  center; padding-top: 25px;}
.footer_info p {margin-bottom: 11px;}
.footer_info .footer_bottom {border-top: 1px solid #565353; clear:  both;}
.footer_info .footer_bottom p {font-size:  16px; margin-bottom: 0px; margin-top:  17px}